"UnlitGeneric"
{
	"$basetexture" "models/player/slow/robocop_chrome/slow_knightrider_scanner"

	"$model" 1

	"$envmap" "env_cubemap"
	"$envmaptint" "[.75 .75 .75]" 
	"$envmapcontrast" 1
	"$envmapsaturation" .5


   "Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
	 "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" 25.00
     	}
	}
}